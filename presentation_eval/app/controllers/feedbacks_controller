class AudienceController < ApplicationController
    def index
        @feedbacks = Feedback.all
    end
    def new
        @feedback = Feedback.new
    end
    def create
        @feedback = Feedback.new(params[:feedback])
        if @feedback.save
            redirect_to presentations_path, :notice => "Thank you"
        else
            render "new"
        end
    end
end